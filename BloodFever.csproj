<Project Sdk="Godot.NET.Sdk/3.3.0">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <RootNamespace>BloodFever</RootNamespace>
    <AssemblyName>BloodFever</AssemblyName>
    <GodotProjectGeneratorVersion>1.0.0.0</GodotProjectGeneratorVersion>
    <TargetFramework>netstandard2.1</TargetFramework>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Global.cs" />
    <Compile Include="actors\**\*.cs" />
    <Compile Include="scenes\**\*.cs" />
    <Compile Include="ui\**\*.cs" />
    <Compile Include="ExportFlagsEnumAttribute.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="addons\**\*.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="BloodFeverFs.fsproj" />
  </ItemGroup>
</Project>